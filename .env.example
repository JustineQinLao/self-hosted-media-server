# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Jellyfin Configuration
JELLYFIN_MEDIA_PATH=./media

# yt-dlp-webui Configuration
# JWT secret for RPC authentication (generate a random secret)
YT_DLP_JWT_SECRET=your_random_jwt_secret_here

# Authentication (optional)
YT_DLP_AUTH=false
YT_DLP_USERNAME=admin
YT_DLP_PASSWORD=your_secure_password

# Download queue size limit (optional, default is unlimited)
YT_DLP_QUEUE_SIZE=0

# Ports Configuration
JELLYFIN_PORT=8096
YT_DLP_PORT=3033
NGINX_PORT=80

# Cloudflare Tunnel Configuration
# Get your token from: https://dash.cloudflare.com/profile/api-tokens
# Or create a tunnel at: https://dash.cloudflare.com/argo-tunnel
CLOUDFLARED_TOKEN=your_cloudflare_tunnel_token_here

# Instructions:
# 1. Copy this file to .env
# 2. Replace placeholder values with your actual credentials
# 3. Never commit the .env file to version control
# 4. Keep .env.example in git to help other developers
